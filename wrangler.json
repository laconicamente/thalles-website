{
  "$schema": "https://developers.cloudflare.com/pages/functions/wrangler-configuration/",
  "name": "thalles-website",
  "compatibility_date": "2024-10-30",
  "build": {
    "command": "npm run build",
    "cwd": "/",
    "watch_dir": "src"
  },
  "routes": [
    {
      "pattern": "/*",
      "custom_domain": true
    }
  ]
}